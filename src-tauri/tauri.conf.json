{
  "productName": "Yaak",
  "version": "0.0.0",
  "identifier": "app.yaak.desktop",
  "build": {
    "beforeBuildCommand": "npm run tauri-before-build",
    "beforeDevCommand": "npm run tauri-before-dev",
    "devUrl": "http://localhost:1420",
    "frontendDist": "../dist"
  },
  "app": {
    "withGlobalTauri": false,
    "security": {
      "assetProtocol": {
        "enable": true,
        "scope": {
          "allow": [
            "$APPDATA/responses/*",
            "$RESOURCE/static/*"
          ]
        }
      }
    }
  },
  "plugins": {
    "deep-link": {
      "desktop": {
        "schemes": [
          "yaak"
        ]
      }
    }
  }
}
